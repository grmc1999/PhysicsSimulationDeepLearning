    "import pandas as pd\n",
    "\n",
    "def solution_data_normalization(u,X,t):\n",
    "    data_dict={\n",
    "        \"u\":u,\n",
    "        \"x\":X[0],\n",
    "        \"y\":X[1],\n",
    "        \"z\":X[2],\n",
    "        \"t\":t if t!=None else [None for _ in X[0]]\n",
    "    }\n",
    "    return pd.DataFrame(data_dict)"
   ]
